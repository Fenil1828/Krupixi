{
  "version": 2,
  "outputDirectory": "Chatbot/dist",
  "buildCommand": "cd Chatbot && npm install && npm run build",
  "installCommand": "echo Skipping root install",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 